{
  "name": "@yaijs/core",
  "version": "1.1.1",
  "description": "Advanced web components with YEH (Yai Event Hub) - the world's first DOM Event Scoping System. Everything you need in one package.",
  "type": "module",
  "main": "./dist/yai-bundle.js",
  "types": "./dist/yai-bundle.d.ts",
  "exports": {
    ".": {
      "types": "./dist/yai-bundle.d.ts",
      "default": "./dist/yai-bundle.js"
    },
    "./bundle": {
      "types": "./dist/yai-bundle.d.ts",
      "default": "./dist/yai-bundle.js"
    },
    "./bundle-core": {
      "types": "./dist/yai-bundle-core.d.ts",
      "default": "./dist/yai-bundle-core.js"
    },
    "./bundle-full": {
      "types": "./dist/yai-bundle-full.d.ts",
      "default": "./dist/yai-bundle-full.js"
    },
    "./core": {
      "types": "./yai-core.d.ts",
      "default": "./yai-core.js"
    },
    "./tabs": {
      "types": "./tabs/yai-tabs.d.ts",
      "default": "./tabs/yai-tabs.js"
    },
    "./swipe": {
      "types": "./utils/yai-tabs-swipe.d.ts",
      "default": "./utils/yai-tabs-swipe.js"
    },
    "./autoswitch": {
      "types": "./utils/yai-auto-switch.d.ts",
      "default": "./utils/yai-auto-switch.js"
    },
    "./viewport": {
      "types": "./utils/yai-viewport.d.ts",
      "default": "./utils/yai-viewport.js"
    },
    "./search-and-click": {
      "types": "./utils/yai-search-and-click.d.ts",
      "default": "./utils/yai-search-and-click.js"
    },
    "./yeh": {
      "types": "./yeh/yeh.d.ts",
      "default": "./yeh/yeh.js"
    }
  },
  "files": [
    "dist/",
    "tabs/",
    "utils/",
    "yeh/",
    "yai-core.js",
    "yai-core.d.ts"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage",
    "demo": "echo \"Open tabs/Example.html in browser for live demo\""
  },
